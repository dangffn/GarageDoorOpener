    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "nodemon": "^3.0.1",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "express": "^4.18.2",
    "onoff": "^6.0.3",
    "opencv-build": "^0.1.9",
    "opencv4nodejs": "^5.6.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tsx": "^4.5.0",
    "typescript": "^5.3.2",
    "vite-express": "*"
  },
  "opencv4nodejs": {
    "autoBuildFlags": "-DOPENCV_GENERATE_PKGCONFIG=ON -DOPENCV_PC_FILE_NAME=opencv.pc",
